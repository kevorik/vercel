{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.https://vercel-pink-nu.vercel.app/heroes/)",
        "dest": "server.js"
      }
    ]
  }